    <div class="notification is-info">
      <div class="buttons is-centered">
        <a href="/login" class="button is-link"><img src="/icons/key-white.svg" class="svg-icon-small" />Login</a>
        <a href="/register" class="button is-link"><img src="/icons/user-plus-white.svg" class="svg-icon-small" />Register</a>
      </div>
    </div>
  </div>
<% } %>
